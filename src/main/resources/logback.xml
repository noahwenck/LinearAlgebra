<configuration>
<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
        <pattern>%d{yyyy-MM-dd HH:mm:ss} [LEARN] [%p] %m%n</pattern>
    </encoder>
</appender>

<logger name="com.wenck" level="INFO"/>

<root level="WARN">
<appender-ref ref="STDOUT"/>
</root>
        </configuration>